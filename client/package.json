	"scripts": {
		"build:main": "eslint -c .eslintrc.json main_module/src/**/*.js main_module/src/**/*.vue --fix && gulp build -m main",
		"build:admin": "eslint -c .eslintrc.json admin_module/src/**/*.js admin_module/src/**/*.vue --fix && gulp build -m admin",
		"build:auth": "eslint -c .eslintrc.json auth_module/src/**/*.js auth_module/src/**/*.vue --fix && gulp build -m auth",
		"build:main:dev": "eslint -c .eslintrc.json main_module/src/**/*.js main_module/src/**/*.vue --fix && gulp build -m main -w",
		"build:admin:dev": "eslint -c .eslintrc.json admin_module/src/**/*.js admin_module/src/**/*.vue --fix && gulp build -m admin -w",
		"build:auth:dev": "eslint -c .eslintrc.json auth_module/src/**/*.js auth_module/src/**/*.vue --fix && gulp build -m auth -w",
		"build:main:prod": "eslint -c .eslintrc.json main_module/src/**/*.js main_module/src/**/*.vue --fix && gulp build -m main --prod",
		"build:admin:prod": "eslint -c .eslintrc.json admin_module/src/**/*.js admin_module/src/**/*.vue --fix && gulp build -m admin --prod",
		"build:auth:prod": "eslint -c .eslintrc.json auth_module/src/**/*.js auth_module/src/**/*.vue --fix && gulp build -m auth --prod",
		"build:all": "eslint -c .eslintrc.json **_module/src/**/*.js **_module/src/**/*.vue --fix && gulp build-all",
		"build:all:prod": "eslint -c .eslintrc.json **_module/src/**/*.js **_module/src/**/*.vue --fix && gulp build-all --prod",
		"test": "echo 'TBD' && exit 0"
	},
