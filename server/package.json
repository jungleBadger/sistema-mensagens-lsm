{
	"name": "sistema-mensagens-lsm",
	"version": "0.0.1",
	"description": "[Backend] - Aplicação para gerenciamento e consumo de mensagens atreladas a eventos.",
	"engineStrict": true,
	"engines": {
		"node": "14.17.0",
		"npm": "6.14.13"
	},
	"main": "./app/dist/app.js",
	"scripts": {
		"start:dev": "nodemon --config ./nodemon.json",
		"start": "cross-env NODE_ENV=development DEBUG=app:* node -r dotenv/config ./app/app.js",
		"start:prod": "cross-env NODE_ENV=production DEBUG=app:main,app:endpoint node -r dotenv/config ./app/app.js",
		"test": "echo 'testing' && exit 0",
		"test-report": "cross-env NODE_ENV=test nyc mocha --exit --timeout=20000",
		"lint": "eslint -c .eslintrc.json app/**.js --fix",
		"documentation:swagger": "gulp generate-swagger-doc",
		"documentation:js": "gulp generate-js-doc"
	},
	"dependencies": {
